#include <iostream>
using namespace std;
class Base {
public:
    void baseMethod() { cout << "Base method\n"; }
};

class Derived1 : public Base {
public:
    void derived1Method() { cout << "Derived1 method\n"; }
};

class Derived2 : public Base {
public:
    void derived2Method() { cout << "Derived2 method\n"; }
};

int main() {
    Derived1 d1;
    Derived2 d2;
    d1.baseMethod();  // Accessible
    d2.baseMethod();  // Accessible
    return 0;
