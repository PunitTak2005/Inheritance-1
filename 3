#include <iostream>
using namespace std;
class Base {
public:
    void baseMethod() { cout << "Base method\n"; }
};

class Intermediate : public Base {
public:
    void intermediateMethod() { cout << "Intermediate method\n"; }
};

class Derived : public Intermediate {
public:
    void derivedMethod() {
        baseMethod();          // Accessible
        intermediateMethod();  // Accessible
    }
};

int main() {
    Derived d;
    d.derivedMethod();
    return 0;
}
