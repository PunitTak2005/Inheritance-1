class Base1 {
public:
    void method1() { cout << "Base1 method\n"; }
};

class Base2 {
public:
    void method2() { cout << "Base2 method\n"; }
};

class Derived : public Base1, public Base2 {
public:
    void accessMethods() {
        method1(); // Accessible
        method2(); // Accessible
    }
};

int main() {
    Derived d;
    d.accessMethods();
    return 0;
}
